	<!-- 要想让跑马灯 循环滚动，必需加 -->		
	<span id="tt" title="88614368" style="display:none;">88614368</span>
	<script type="text/javascript"> 
		/* 数每秒加1 */
		$(function (){
			reduceTime();
			});
		function reduceTime() {
				var reduce = $("#tt").html();
				++reduce;
				var sec = reduce;
	
				var atop = $("#tt").attr("title");
				atop = atop-86400+86400*2;
				if(reduce != atop){
						$("#tt").html(sec);
				}else{
						$("#tt").html(reduce);
						return reduce;
				}
				setTimeout(reduceTime,2000);
		}
