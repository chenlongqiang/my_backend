* {
padding: 0;
margin: 0;
}

body {
font-family: Microsoft YaHei;
font-size: 12px;
background: #fff;
color:#000;
}
input,textarea,select,button {
outline: none;
font-family: Microsoft YaHei;
}
a img {
border: none;
}

li {
list-style: none;
}

a {
text-decoration: none;
}

a {
-webkit-tap-highlight-color: rgba(0,0,0,0);
-webkit-touch-callout: none;
-webkit-user-select: none;
}


h1,h2,h3,h4,h5 {
font-weight: normal;
}

.clear {
clear: both;
}



#m-body {
